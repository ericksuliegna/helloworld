from linkedlist import LinkedList

class Stack:
    def __init__(self):
        self.stack = LinkedList()

    def push(self, value):
        if self.stack.length == 0:
            self.stack.add(value)
        else:
            self.stack.insert(0, value)

    def pop(self):
        if self.stack.length == 0:
            return None
        else:
            pointer = self.stack.get(0)
            self.stack.remove(0)
            return pointer

    def __len__(self):
        return self.stack.length


